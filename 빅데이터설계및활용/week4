s = 1
for i in range(1,10):
  while s <= 9:
    print("%s*%s=%s" % (i, s, i*s))
    s = s + 1
  s = 1

/////////////////////////////////////////

def getMinAndMax(mylist):
  min_value = mylist[0]
  max_value = mylist[0]

  for value in mylist:
    if value > max_value:
      max_value = value
    if value < min_value:
      min_value = value
  return max_value, min_value

list_data = [82, 27, 90, 30, 87, 56]
print(list_data)
print("최대값 : %d \n최소값 : %d" % getMinAndMax(list_data))
